{
  "name": "blogback",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@eslint/js": "^9.38.0",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prisma": "^7.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "bun --watch src/server.ts",
    "format": "eslint . --fix && prettier --write .",
    "build": "bun build src/server.ts --outdir dist --target bun",
    "test": "bun test",
    "migrate": "bunx prisma migrate dev",
    "seed": "bunx prisma db seed",
    "generate": "bunx prisma generate",
    "reset": "bunx prisma migrate reset --force",
    "drop": "bunx prisma db push --force-reset"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "elysia": "^1.4.13",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prismabox": "^1.1.25",
    "slugify": "^1.6.6",
    "xss": "^1.0.15"
  }
}
